generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Accounts {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model Users {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  email    String @unique
  password String
  username String @unique
}
